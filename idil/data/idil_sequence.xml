<?xml version="1.0" encoding="UTF-8"?>
<odoo>
    <data noupdate="1">
        <!-- Sequence for Manufacturing Order Reference -->
        <record id="seq_idil_manufacturing_order" model="ir.sequence">
            <field name="name">Manufacturing Order Sequence</field>
            <field name="code">idil.manufacturing.order.sequence</field>
            <field name="prefix">MO/%(year)s/</field>
            <field name="number_increment">1</field>
            <field name="padding">5</field> <!-- Sequence number padding, e.g., 00001 -->
            <field name="company_id" eval="False"/> <!-- Apply to all companies; set specific company ID if needed -->
        </record>
        <!-- Sequence for Manufacturing Order Reference -->
        <record id="seq_idil_sale_order" model="ir.sequence">
            <field name="name">Sales Order Sequence</field>
            <field name="code">idil.sale.order.sequence</field>
            <field name="prefix">SO/%(year)s/</field>
            <field name="number_increment">1</field>
            <field name="padding">5</field> <!-- Sequence number padding, e.g., 00001 -->
            <field name="company_id" eval="False"/> <!-- Apply to all companies; set specific company ID if needed -->
        </record>
         
        <record id="seq_item_opening_balance" model="ir.sequence">
            <field name="name">Item Opening Balance</field>
            <field name="code">idil.item.opening.balance</field>
            <field name="prefix">IOB/%(year)s/</field>
            <field name="padding">5</field>
        </record>

        <record id="seq_stock_adjustment" model="ir.sequence">
            <field name="name">Stock Adjustment</field>
            <field name="code">idil.stock.adjustment.sequence</field>
            <field name="prefix">St-Adj/%(year)s/</field>
            <field name="padding">5</field>
            <field name="number_next">1</field>
            <field name="number_increment">1</field>
        </record>
        
        <record id="seq_commission_bulk_payment" model="ir.sequence">
            <field name="name">Commission Bulk Payment</field>
            <field name="code">idil.commission.bulk.payment.seq</field>
            <field name="prefix">CBP/%(year)s/</field>
            <field name="padding">5</field>
            <field name="number_next">1</field>
            <field name="number_increment">1</field>
        </record>

        <record id="seq_sales_opening_balance" model="ir.sequence">
            <field name="name">Sales Opening Balance</field>
            <field name="code">idil.sales.opening.balance</field>
            <field name="prefix">SOB/%(year)s/</field>
            <field name="padding">5</field>
            <field name="company_id" eval="False"/>
        </record>
            <!-- Sequence for Customer Opening Balance -->
        <record id="seq_idil_customer_opening_balance" model="ir.sequence">
            <field name="name">Customer Opening Balance</field>
            <field name="code">idil.customer.opening.balance</field>
            <field name="prefix">COB/%(year)s/</field>
            <field name="padding">5</field>
            <field name="company_id" eval="False"/>
        </record>
    
        <record id="seq_idil_receipt_bulk_payment" model="ir.sequence">
            <field name="name">Bulk Receipt Payment</field>
            <field name="code">idil.receipt.bulk.payment.seq</field>
            <field name="prefix">BRP/%(year)s/</field>
            <field name="padding">5</field>
        </record>
        
        <record id="seq_idil_vendor_opening_balance" model="ir.sequence">
            <field name="name">Vendor Opening Balance</field>
            <field name="code">idil.vendor.opening.balance</field>
            <field name="prefix">VOB/%(year)s/</field>
            <field name="padding">5</field>
            <field name="company_id" eval="False"/>
            <field name="active">True</field>
        </record>
        
        <record id="seq_customer_sale_return" model="ir.sequence">
            <field name="name">Customer Sale Return</field>
            <field name="code">idil.customer.sale.return</field>
            <field name="prefix">CSR/%(year)s/</field>
            <field name="padding">5</field>
            <field name="number_next">1</field>
            <field name="number_increment">1</field>
        </record>

        <record id="seq_product_opening_balance" model="ir.sequence">
            <field name="name">Product Opening Balance</field>
            <field name="code">my_product.opening.balance</field>
            <field name="prefix">POB/%(year)s/</field>
            <field name="padding">5</field>
            <field name="number_next">1</field>
            <field name="number_increment">1</field>
        </record>

        <record id="seq_salereturn" model="ir.sequence">
            <field name="name">idil sale return</field>
            <field name="code">idil.sale.return</field>
            <field name="prefix">SR/%(year)s/ </field>
            <field name="padding">5</field>
            <field name="number_next">1</field>
            <field name="number_increment">1</field>
        </record>

        <record id="seq_staff_sales" model="ir.sequence">
            <field name="name">Staff Sales</field>
            <field name="code">idil.staff.sales</field>
            <field name="prefix">SS/</field>
            <field name="padding">4</field>
            <field name="company_id" eval="False"/>
        </record>
        <record id="seq_customer_place_order" model="ir.sequence">
            <field name="name">Customer Place Order</field>
            <field name="code">idil.customer.place.order.sequence</field>
            <field name="prefix">ORD-</field>
            <field name="padding">5</field>
            <field name="implementation">standard</field>
        </record>

    </data>
</odoo>
